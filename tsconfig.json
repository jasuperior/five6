{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "moduleResolution": "Node",
        "strict": false,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "jsx": "react",
        "jsxFactory": "Five6.createElement",
        "jsxFragmentFactory": "Five6.createFragment",
        "typeRoots": ["./src/lib/model"]
    },
    "include": ["src"]
}
